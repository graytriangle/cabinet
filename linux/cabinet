server {
	listen 443 ssl;
	listen [::]:443 ssl;
	include snippets/self-signed.conf;
	include snippets/ssl-params.conf;
	server_name 192.168.56.1;

	location / {
		include uwsgi_params;
		uwsgi_pass unix:/home/user/web/cabinet.sock;
	}
}

server {
    listen 80;
    listen [::]:80;

    server_name 192.168.56.1;

    return 301 https://$server_name$request_uri;
}